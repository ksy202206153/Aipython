      "source": [
        "#반지름이 5인 원의 면적을 구하시오\n",
        "#파이 값은 3.141592로 계산합니다\n",
        "r=5\n",
        "pi=3.141592\n",
        "area = pi*r*r\n",
        "\n",
        "print(f\"반지름이 {r}인 원의 면적은 {area} 입니다.\")\n"
      ]
    },
